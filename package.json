{
  "name": "omni-jimmer-tool",
  "version": "1.0.0",
  "description": "Matrix-based AI video generator for Brazilian businesses with CNAE classification",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8080",
    "dev": "python3 -m http.server 3000",
    "proxy": "node proxy-server.js",
    "dev-full": "concurrently \"npm run start\" \"npm run proxy\"",
    "build": "./build-render.sh",
    "deploy": "rsync -avz --exclude node_modules --exclude .git . user@server:/path/to/deployment/",
    "lint": "echo 'Linting HTML, CSS, and JS files...'",
    "test": "echo 'No tests configured yet'"
  },
  "keywords": [
    "ai",
    "video-generator",
    "brazilian-business",
    "cnae",
    "matrix",
    "content-generation"
  ],
  "author": "Omni Jimmer Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/omni-jimmer-tool.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/omni-jimmer-tool/issues"
  },
  "homepage": "https://github.com/your-username/omni-jimmer-tool#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
